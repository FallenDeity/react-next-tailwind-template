{
	"name": "nextjs-template",
	"version": "1.0.0",
	"description": "A template for Next.js projects",
	"main": "src/index.ts",
	"rootDir": "src",
	"scripts": {
		"test": "mocha --require ts-node/register tests/**/*.ts",
		"lint": "eslint src",
		"lint:fix": "eslint --fix src",
		"prettier": "prettier .",
		"prettier:fix": "prettier --write .",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"coverage": "nyc npm run test"
	},
	"pre-commit": [
		"lint:fix",
		"prettier:fix",
		"build",
		"test",
		"coverage"
	],
	"keywords": [],
	"author": "FallenDeity",
	"license": "MIT",
	"devDependencies": {
		"@eslint/compat": "^1.1.1",
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.11.1",
		"@next/eslint-plugin-next": "^14.2.14",
		"@types/mocha": "^10.0.8",
		"@types/node": "^22.7.4",
		"@typescript-eslint/eslint-plugin": "^8.8.0",
		"@typescript-eslint/parser": "^8.8.0",
		"autoprefixer": "^10.4.20",
		"eslint": "8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.30.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"globals": "^15.10.0",
		"mocha": "^10.7.3",
		"nyc": "^17.1.0",
		"postcss": "^8.4.47",
		"pre-commit": "^1.2.2",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"tailwindcss": "^3.4.13",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.2",
		"webpack": "^5.95.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"@types/react": "^18.3.10",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"lucide-react": "^0.447.0",
		"next": "^14.2.14",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tailwind-merge": "^2.5.2",
		"tailwindcss-animate": "^1.0.7",
		"winston": "^3.14.2"
	},
	"prettier": {
		"trailingComma": "es5",
		"tabWidth": 4,
		"useTabs": true,
		"semi": false,
		"printWidth": 120,
		"bracketSpacing": true,
		"bracketSameLine": true,
		"endOfLine": "auto",
		"plugins": ["prettier-plugin-tailwindcss"]
	}
}
